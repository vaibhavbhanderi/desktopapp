{
  "name": "angular-desktop",
  "version": "0.0.0",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve && electron .",
    "build": "ng build",
    "electron": "ng build && electron .",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "start:electron": "ng build --base-href ./ && electron .",
    "winpackage": "electron-packager . DemoApp --platform=win32 --arch=x64 --overwrite --app-version=1.1.0 --build-version=1.2.0 --win32metadata.ProductName=DemoApp --FileDescription=DemoApp --appname=demoapp --win32metadata.CompanyName=ng --app-copyright=ng --icon=eagle.ico",
    "archiveapp": "asar DemoApp-win32-x64/resources/app DemoApp-win32-x64/resources/app.asar",
    "pcackit": "ng build --base-href ./ && npm run winpackage && npm run archiveapp && electron ."
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.1.0",
    "@angular/common": "^15.1.0",
    "@angular/compiler": "^15.1.0",
    "@angular/core": "^15.1.0",
    "@angular/forms": "^15.1.0",
    "@angular/platform-browser": "^15.1.0",
    "@angular/platform-browser-dynamic": "^15.1.0",
    "@angular/router": "^15.1.0",
    "@types/electron": "^1.6.10",
    "asar": "^3.2.0",
    "browserify-fs": "^1.0.0",
    "child_process": "^1.0.2",
    "electron-fs-extra": "^1.0.0",
    "electron-packager": "^17.1.1",
    "electron-reload": "^2.0.0-alpha.1",
    "events": "^3.3.0",
    "fs": "^0.0.1-security",
    "fs-web": "^1.0.1",
    "lodash-es": "^4.17.21",
    "ngx-electron": "^2.2.0",
    "path": "^0.12.7",
    "path-browserify": "^1.0.1",
    "rxjs": "~7.8.0",
    "stream": "^0.0.2",
    "timers": "^0.1.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.1.1",
    "@angular/cli": "~15.1.1",
    "@angular/compiler-cli": "^15.1.0",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^18.11.19",
    "assert": "^2.0.0",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.0",
    "electron": "^22.2.0",
    "https-browserify": "^1.0.0",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "node-polyfill-webpack-plugin": "^2.0.1",
    "os-browserify": "^0.3.0",
    "process": "^0.11.10",
    "react-app-rewired": "^2.2.1",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "typescript": "~4.9.4",
    "url": "^0.11.0"
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false,
    "child_process": false
  }
}
